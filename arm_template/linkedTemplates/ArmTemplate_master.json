{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "factoryName": {
            "type": "string",
            "metadata": "Data Factory name",
            "defaultValue": "team06datafactory"
        },
        "SqlServer1_connectionString": {
            "type": "secureString",
            "metadata": "Secure string for 'connectionString' of 'SqlServer1'"
        },
        "CosmosDbNoSql1_connectionString": {
            "type": "secureString",
            "metadata": "Secure string for 'connectionString' of 'CosmosDbNoSql1'"
        },
        "AzureDataLakeStorage1_accountKey": {
            "type": "secureString",
            "metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
        },
        "AzureSqlDatabase1_connectionString": {
            "type": "secureString",
            "metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
        },
        "challenge2_accountKey": {
            "type": "secureString",
            "metadata": "Secure string for 'accountKey' of 'challenge2'"
        },
        "AzureDataLakeStorage1_properties_typeProperties_url": {
            "type": "string",
            "defaultValue": "https://covid19ggh7nlmcuiou6.dfs.core.windows.net/"
        },
        "challenge2_properties_typeProperties_url": {
            "type": "string",
            "defaultValue": "https://covid19ggh7nlmcuiou6.dfs.core.windows.net/"
        },
        "containerUri": {
            "type": "string",
            "metadata": "URI to storage account containing linked ARM templates"
        },
        "containerSasToken": {
            "type": "string",
            "metadata": "Sas token to storage account containing linked ARM templates"
        }
    },
    "variables": {
        "factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
    },
    "resources": [
        {
            "name": "team06datafactory_ArmTemplate_0",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2018-02-01",
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "factoryName": {
                        "value": "[parameters('factoryName')]"
                    },
                    "AzureDataLakeStorage1_accountKey": {
                        "value": "[parameters('AzureDataLakeStorage1_accountKey')]"
                    },
                    "AzureSqlDatabase1_connectionString": {
                        "value": "[parameters('AzureSqlDatabase1_connectionString')]"
                    },
                    "challenge2_accountKey": {
                        "value": "[parameters('challenge2_accountKey')]"
                    },
                    "SqlServer1_connectionString": {
                        "value": "[parameters('SqlServer1_connectionString')]"
                    },
                    "CosmosDbNoSql1_connectionString": {
                        "value": "[parameters('CosmosDbNoSql1_connectionString')]"
                    },
                    "AzureDataLakeStorage1_properties_typeProperties_url": {
                        "value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
                    },
                    "challenge2_properties_typeProperties_url": {
                        "value": "[parameters('challenge2_properties_typeProperties_url')]"
                    }
                }
            },
            "dependsOn": []
        },
        {
            "name": "team06datafactory_ArmTemplate_1",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2018-02-01",
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "factoryName": {
                        "value": "[parameters('factoryName')]"
                    }
                }
            },
            "dependsOn": [
                "[resourceId('Microsoft.Resources/deployments', 'team06datafactory_ArmTemplate_0')]"
            ]
        }
    ]
}